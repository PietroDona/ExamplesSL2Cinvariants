      RowBox[{"Sqrt", "[", 
       RowBox[{
        SuperscriptBox["j", "2"], "+", 
        RowBox[{
         SuperscriptBox["\[Gamma]", "2"], 
         SuperscriptBox["k", "2"]}]}], "]"}]], 
     RowBox[{"Exp", "[", 
      RowBox[{"I", " ", 
       RowBox[{"ArcTan", "[", 
        RowBox[{"\[Gamma]", " ", 
         RowBox[{"k", "/", "j"}]}], "]"}]}], "]"}], " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"F2", "+", 
        RowBox[{"I", " ", "nF2"}]}], ")"}], ".", "Rz\[Psi]", ".", 
