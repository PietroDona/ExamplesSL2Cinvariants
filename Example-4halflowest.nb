Cell[BoxData[{
 RowBox[{"atmbt1", "=", 
  RowBox[{
   RowBox[{"Arg", "[", 
    RowBox[{"j", " ", 
     RowBox[{"Sign", "[", "r1", "]"}], 
     FractionBox[
      RowBox[{"Sqrt", "[", 
       RowBox[{"1", "+", 
