         SuperscriptBox["\[Gamma]", "2"], 
         SuperscriptBox["k", "2"]}]}], "]"}]], 
     RowBox[{"Exp", "[", 
      RowBox[{"I", " ", 
       RowBox[{"ArcTan", "[", 
        RowBox[{"\[Gamma]", " ", 
         RowBox[{"k", "/", "j"}]}], "]"}]}], "]"}], " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"F1", "+", 
        RowBox[{"I", " ", "nF1"}]}], ")"}], ".", "Rz\[Psi]", ".", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Cross", "[", 
         RowBox[{"z", ",", "n1"}], "]"}], "/", 
        RowBox[{"Norm", "[", 
         RowBox[{"Cross", "[", 
          RowBox[{"z", ",", "n1"}], "]"}], "]"}]}], ")"}]}]}], "]"}], "//", 
   "FullSimplify"}]}], "\[IndentingNewLine]", 
 RowBox[{"atmbt2", "=", 
  RowBox[{
   RowBox[{"Arg", "[", 
